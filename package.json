{
  "name": "lil-orm",
  "version": "3.0.0",
  "description": "Lil ORM is a lightweight and flexible ORM for Node.js, designed with a clear and intuitive API to simplify database operations. Engineered for adaptability, it enables efficient interaction with databases.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "orm",
    "database",
    "nodejs",
    "node-orm",
    "postgresql",
    "sqlite",
    "typescript-orm",
    "javascript-orm",
    "database-mapping",
    "data-modeling",
    "query-builder",
    "sql",
    "database-abstraction",
    "lightweight-orm",
    "flexible-orm",
    "modern-web-development",
    "api-development",
    "nodejs-database"
  ],
  "author": "Scassius",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.202",
    "@types/pg": "^8.11.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "date-fns": "^3.3.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  },
  "optionalDependencies": {
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    }
  }
}
